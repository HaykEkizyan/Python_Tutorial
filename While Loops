# 1

i = 1
while i < 5:
    print(i)
    i += 1   
""" 
    1
    2
    3
    4  
"""
    
    
# 2

i = 1
while i <= 5:
    print('*' * i)
    i += 1
    
""" 
    *
    **
    ***
    ****
    ***** 
"""
    
# 3

i = 11
while i >= 1:
    print(f"{'*' * i}")
    i -= 2
    
"""
    ***********
    *********
    *******
    *****
    ***
    *
"""


