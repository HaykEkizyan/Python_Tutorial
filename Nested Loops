for x in range(2):
    for y in range(2):
        print(f'({x}, {y})')
        
       """
        (0, 0)
        (0, 1)
        (1, 0)
        (1, 1)
       """
       
       
#2 Printing "O" 
  
numbers = [5, 3, 1, 2, 4]
for i in numbers:
    print("O" * i)
   """
    OOOOO
    OOO
    O
    OO
    OOOO
   """
